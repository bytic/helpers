{
  "name": "bytic/helpers",
  "description": "Php Framework",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Helpers\\": "src/"
    },
    "classmap": [
      "src",
      "legacy"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Helpers\\Tests\\": "tests/src",
      "Nip\\Helpers\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.2 || ^8.0",
    "ext-json": "*",
    "bytic/assets": "^1.0",
    "bytic/config": "^0.9|^1.0",
    "bytic/filesystem": "^0.9|^1.0",
    "bytic/flash": "^0.9|^1.0",
    "bytic/router": "^0.9|^1.0",
    "bytic/utility": "^0.9|^1.0",
    "bytic/google-analytics": "^1.0"
  },
  "require-dev": {
    "bytic/http": "^0.9|^1.0",
    "phpunit/phpunit": "^7.2|^8.0|^9.0",
    "mockery/mockery": "^1.1"
  }
}
