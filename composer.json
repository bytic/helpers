{
  "name": "bytic/helpers",
  "description": "Php Framework",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Helpers\\": "src/"
    },
    "classmap": [
      "src",
      "legacy"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Helpers\\Tests\\": "tests/src",
      "Nip\\Helpers\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "bytic/utility": "^1.0|^2.0"
  },
  "require-dev": {
    "bytic/assets": "^1.0|^2.0",
    "bytic/seo-meta": "^1.0|^2.0",
    "bytic/config": "^1.0|^2.0",
    "bytic/filesystem": "^1.0|^2.0",
    "bytic/flash": "^1.0|^2.0",
    "bytic/http": "^0.9|^1.0",
    "bytic/router": "^1.0|^2.0",
    "bytic/google-analytics": "^1.0|^2.0",
    "bytic/phpqatools": "^1.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
